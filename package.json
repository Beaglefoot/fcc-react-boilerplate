    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-immutable": "^1.6.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "date-fns": "^1.28.5",
    "dumper": "^1.0.12",
