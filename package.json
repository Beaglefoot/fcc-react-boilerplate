{
  "name": "fcc-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "env NODE_ENV=test mocha --require babel-core/register --require ./test/setup.js \"{test,src}/**/*_test.@(js|jsx)\" --colors",
    "test:watch": "nodemon -e js,jsx -w src -w test --exec 'clear && yarn test'",
    "build": "webpack --config ./config/webpack.prod.config.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "analyze": "webpack --config ./config/webpack.analyze.config.js",
    "fix:assets": "prettier --ignore-path .gitignore --write \"**/*.{json,md,scss}\"",
    "fix:code": "yarn lint --fix",
    "fix": "yarn fix:code && yarn fix:assets",
    "lint": "tslint './src/**/*.{ts,tsx}' -p tsconfig.json"
  },
  "lint-staged": {
    "*.{json,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -p tsconfig.json",
      "git add"
    ]
  },
  "author": "Beaglefoot",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^6.0.4",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "date-fns": "^1.28.5",
    "dumper": "^1.0.12",
    "enzyme": "^3.3.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "^4.3.1",
    "jsdom": "^11.11.0",
    "lint-staged": "7.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.0",
    "prettier": "1.13.7",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "sinon": "^6.1.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  }
}
