  "main": "index.js",
  "scripts": {
    "test": "env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js \"{test,src}/**/*_test.@(js|jsx)\" --colors",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "build": "node ./node_modules/webpack/bin/webpack.js --config ./config/webpack.prod.config.js",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./config/webpack.dev.config.js",
    "analyze": "node ./node_modules/webpack/bin/webpack.js --config ./config/webpack.analyze.config.js"
  },
